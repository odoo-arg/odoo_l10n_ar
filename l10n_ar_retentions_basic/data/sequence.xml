<openerp>

    <data noupdate="0">

        <!-- SECUENCIA PARA RETENTION GANANCIA -->
        <record model="ir.sequence.type" id="retention_profit_sequence_type">
            <field name="name">Payment Retention Profit Code</field>
            <field name="code">rtl.profit.seq</field>
        </record>

        <record model="ir.sequence" id="retention_profit_sequence">
            <field name="name">Payment Retention Profit Sequence</field>
            <field name="code">rtl.profit.seq</field>
            <field name="active">True</field>
            <field name="prefix">0000-%(year)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">6</field>
            <field name="implementation">no_gap</field>
        </record>

        <!-- SECUENCIA PARA RETENTION IVA -->
        <record model="ir.sequence.type" id="retention_vat_sequence_type">
            <field name="name">Payment Retention Vat Code</field>
            <field name="code">rtl.vat.seq</field>
        </record>

        <record model="ir.sequence" id="retention_vat_sequence">
            <field name="name">Payment Retention Vat Sequence</field>
            <field name="code">rtl.vat.seq</field>
            <field name="active">True</field>
            <field name="prefix">0001-</field>
            <field name="number_increment">1</field>
            <field name="padding">8</field>
            <field name="implementation">no_gap</field>
        </record>

        <!-- SECUENCIA PARA RETENTION IIBB -->
        <record model="ir.sequence.type" id="retention_gross_sequence_type">
            <field name="name">Payment Retention Gross Income Code</field>
            <field name="code">rtl.gross.seq</field>
        </record>

        <record model="ir.sequence" id="retention_gross_sequence">
            <field name="name">Payment Retention Gross Income Sequence</field>
            <field name="code">rtl.gross.seq</field>
            <field name="active">True</field>
            <field name="prefix">0002-%(year)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">6</field>
            <field name="implementation">no_gap</field>
        </record>

    </data>

</openerp>

